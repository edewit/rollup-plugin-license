{{#each modules}}
<other>
    <description>{{this.packageJson.name}}</description>
    <locations>
    {{#each files}}
        {{#if ../truncated}}
        <directory>{{this}}</directory>
        {{else}}
        <file>{{this}}</file>
        {{/if}}
    {{/each}}
    </locations>
    <licenses>
    <license>
        <name>{{this.packageJson.license}}</name>
        <url>{{licenseUrl this.packageJson.repository.url}}</url>
    </license>
    </licenses>
</other>
{{/each}}